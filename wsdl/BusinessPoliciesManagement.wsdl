									<DeprecationDetails>NoOp</DeprecationDetails>
									<EndOfLifeVersion>1139</EndOfLifeVersion>
									<requiredInput>Conditionally</requiredInput>
								</callInfo>
								<callInfo>
									<NoCalls/>
									<DeprecationVersion>1061</DeprecationVersion>
									<DeprecationDetails>NoOp</DeprecationDetails>
									<EndOfLifeVersion>1139</EndOfLifeVersion>
									<returned>Conditionally</returned>
